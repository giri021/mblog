{
  "containerDefinitions": [
    {
      "name": "giri-alpha-mblog-pro",
      "image": "081602682094.dkr.ecr.us-east-2.amazonaws.com/giri-alpha-mblog-pro:latest",
      "cpu": 0,
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 80
        }
      ],
      "dockerLabels" : {
        "container" : "giri-alpha-mblog-pro",
        "environment" : "Production"
      },
      "logConfiguration" : {
        "logDriver": "json-file",
        "options": {
          "labels" : "container,environment"
        }
      }
    }]
}
